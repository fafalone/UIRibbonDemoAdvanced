            Case IDC_CMD_CONTEXT2: Return IDC_CMD_CONTEXTMAP2
            Case IDC_CMD_CONTEXT3: Return IDC_CMD_CONTEXTMAP3
            Case IDC_CMD_CONTEXT4: Return IDC_CMD_CONTEXTMAP4
        End Select
